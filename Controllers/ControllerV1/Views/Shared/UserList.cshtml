@model IEnumerable<ControllerV1.Models.User>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserList</title>
</head>
<body>
    <div>       
        User List
        @if (Model.Count() != 0)
        {
            <ul>
                @foreach (var user in Model)
                {
                    <li>@user.Name</li>
                }
            </ul>
        }
        else
        {
            <p>No users</p>
        }
    </div>
</body>
</html>
